{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [\r\n    {\r\n      "src": "/dashboard",\r\n      "dest": "/api/index.js"\r\n    },\r\n    {\r\n      "src": "/dashboard/(.*)",\r\n      "dest": "/api/index.js"\r\n    },\r\n    {\r\n      "src": "/dashboard-modern",\r\n      "dest": "/api/index.js"\r\n    },\r\n    {\r\n      "src": "/dashboard-modern/(.*)",\r\n      "dest": "/api/index.js"\r\n    },
    {
      "src": "/api/health",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/pages",
      "dest": "/api/index.js"
    },
    {
      "src": "/app.*",
      "dest": "/api/index.js"
    },
    {
      "src": "/app/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/builder/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/install",
      "dest": "/public/install.html"
    },
    {
      "src": "/editor",
      "dest": "/public/editor.html"
    },
    {
      "src": "/analytics",
      "dest": "/public/analytics.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "SHOPIFY_APP_URL": "https://kingsbuilder.vercel.app",
    "FRONTEND_URL": "https://kingsbuilder.vercel.app",
    "CORS_ORIGIN": "https://admin.shopify.com,https://*.myshopify.com,https://kingsbuilder.vercel.app",
    "COOKIE_SECURE": "true",
    "COOKIE_SAME_SITE": "none"
  }
}

